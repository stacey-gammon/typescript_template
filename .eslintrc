{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "quotes": ["error", "single"],
      "indent": ["error", 2],
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": ["error"]
    }
}
